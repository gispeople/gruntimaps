{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Container": {
    "Fonts": "fonts",
    "Geojsons": "geojsons",
    "MbTiles": "mbtiles",
    "Packs": "packs",
    "Overlays": "overlays",
    "Styles": "styles"
  },
  "Queue": {
    "MvtConversion": "mb-conversion",
    "GdalConversion": "gd-conversion"
  },
  "Table": {
    "JobStatuses": "jobStatuses"
  },
  "Path": {
    "Root": "/gruntimaps-data",
    "Fonts": "/gruntimaps-data/fonts",
    "Packs": "/gruntimaps-data/packs",
    "Styles": "/gruntimaps-data/styles",
    "Tiles": "/gruntimaps-data/tiles"
  },
  "Provider": {
    "Type": "Local",
    "Local": {
      "Path": "/gruntimaps-data/containers",
      "QueueEntryLife": 5,
      "QueueTimeLimit": 30
    },
    "Azure": {
      "ConnectionString": ""
    }
  },
  "Service": {
    "LayerRefresh": 10,
    "ConvertPolling": 10
  },
  "Authentication": {
    "AdminToken": ""
  }
}
