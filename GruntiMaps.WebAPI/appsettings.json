{
  "AllowedHosts": "*",
  "Containers": {
    "fonts": "fonts",
    "geojson": "geojson",
    "mbtiles": "mbtiles",
    "packs": "packs",
    "storage": "overlays",
    "styles": "styles"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Paths": {
    "contentRoot": "/gruntimaps-data",
    "fontDir": "/gruntimaps-data/fonts",
    "packDir": "/gruntimaps-data/zip",
    "styleDir": "/gruntimaps-data/styles",
    "tileDir": "/gruntimaps-data/tiles"
  },
  "Provider": {
    "Type": "local",
    "LocalSettings": {
      "Path": "/gruntimaps-data/containers",
      "queueEntryLife": 5,
      "queueTimeLimit": 30
    },
    "AzureSettings": {
      "globalStorageAccount": "",
      "globalStorageKey": ""
    }
  },
  "Queues": {
    "mvtConversion": "mb-conversion",
    "gdalConversion": "gd-conversion"
  },
  "Tables": {
    "jobStatus":  "jobStatuses" 
  },
  "Services": {
    "layerRefresh": 10,
    "convertPolling": 10
  }
}
